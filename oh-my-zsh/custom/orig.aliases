pvcp() {
	# a better cp with pv; http://jrenard.info/blog/a-better-cp-with-pv.html
	SOURCE=$1
	
	if [ -d $2 ]
	then
	    DESTINATION=$2/`basename $SOURCE`
	else
	    DESTINATION=$2
	fi
	
	pv ${SOURCE} | > ${DESTINATION}
}

